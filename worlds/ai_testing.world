<?xml version="1.0" encoding="utf-8"?>
<sdf version="1.4">
  <world name="default">
  	<physics type="ode">
	  <max_step_size>.003</max_step_size>
	  <real_time_factor>1</real_time_factor>
	  <!--<real_time_update_rate>100</real_time_update_rate>-->
	  <ode>
		<solver>
		  <type>quick</type>
		  <iters>100</iters>
		</solver>
	  </ode>
	</physics>
    <include>
      <uri>model://sun</uri>
    </include>
    <include>
      <uri>model://ground_plane</uri>
    </include>

<!--    <include>
      <uri>model://robot-Insomnia</uri>
      <pose>0 0 1 1.57 0 0</pose>
    </include>-->
    <include>
      <uri>model://robot-Ascent</uri>
      <pose>0 -0.5 0.7 0 0 1.57</pose>
    </include>
	<include>
	  <uri>model://ai_test</uri>
	  <pose>0 0 0.7 0 0 0</pose>
	</include>

<model name="camGround">
     <pose>0 0 0 0 0 0</pose> 
	<static>true</static>    
	 <link name="camGround">
       <!--<visual name ='visual'>\
         <geometry>
           <box><size>1 1 1</size></box>
         </geometry>
       </visual>\-->
       <sensor name="worldCamera" type="camera">
           <pose>0 0 3 0 0 0</pose>
             <camera name="saveCamera">
               <horizontal_fov>1</horizontal_fov>
               <image>
                 <format>R8G8B8</format>
                 <width>1920</width>
                 <height>1080</height>
               </image>
               <clip>
                 <near>0.1</near>
                 <far>250</far>
               </clip>
               <save enabled=true>
                 <path>Captures/2017_02_27</path>
               </save>
             </camera>
             <always_on>1</always_on>
             <update_rate>30</update_rate>
         <visualize>true</visualize>
       </sensor>
     </link>
   </model>

    <model name="box">
    <static>true</static>
      <pose>0 0 0 0 0 0</pose>\
      <link name ='box'>\
        <pose>0 0 0 0 0 0</pose>\
        <visual name ='visual'>\
          <geometry>
            <box><size>20 20 .5</size></box>
          </geometry>
        </visual>\
        <collision name ='visual'>\
          <geometry>
            <box><size>20 20 .5</size></box>
          </geometry>
          <surface>
            <friction>
              <ode>
                <mu>0.2</mu>
                <mu2>0.2</mu2>
              </ode>
            </friction>
          </surface>
        </collision>\
      </link>
    </model>
  </world>
</sdf>
